    "json-bigint": "^1.0.0",
    "nestjs-pino": "^4.0.0",
    "nft.storage": "^7.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "pinata": "^2.1.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "socket.io": "^4.7.1",
    "typeorm": "^0.3.20",
    "winter-cardano-mesh": "^0.0.37-alpha"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.4",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "18.16.12",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-strategy": "^0.2.38",
    "@types/qs": "^6.9.18",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.8",
    "@types/uuid4": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
